{
  "BitmapFont": {
    default-font: {
      file: fonts/font-small-export.fnt
    },
    font-title: {
      file: fonts/font-title-export.fnt
    }
  },
  "ButtonStyle": {
    emphasis-colored: {
      up: button-emphasis-color
    }
    emphasis: {
      up: button-emphasis
    }
  },
  "Color": {
    white: {
      a: 1,
      b: 1,
      g: 1,
      r: 1
    },
    black: {
      a: 1,
      b: 0,
      g: 0,
      r: 0
    }
  },
  "TextButtonStyle": {
    default: {
      down: file-menu-button,
      up: file-menu-button,
      over: file-menu-button
      font: default-font,
      fontColor: white,
      downFontColor: black
    },
    emphasis-textbutton: {
      font: default-font,
      fontColor: white,
      overFontColor: black,
      disabledFontColor: white,
      up: button-emphasis
    }
  },
  LabelStyle: {
    title-label: {
      font: font-title
      fontColor: white
    }
  }
}